{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.exclude": {
    "node_modules/": true,
    "package-lock.json": true,
    ".next": true,
    ".gitignore": true,
    "next.config.js": true,
    "package.json": true,
    ".eslintrc": true,
    ".prettierrc": true
  }
}
